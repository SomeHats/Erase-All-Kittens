{
  "name": "web-platform",
  "version": "0.0.0",
  "ignore": [
    "**/.*",
    "node_modules",
    "components"
  ],
  "dependencies": {
    "async": "0.9.0",
    "backbone": "latest",
    "codemirror": "5.9.0",
    "fastclick": "1.0.3",
    "font-awesome": "4.1.0",
    "handlebars": "1.3.0",
    "jquery": "2.1.3",
    "modernizr": "2.8.3",
    "nouislider": "6.2.0",
    "platform": "1.2.0",
    "popcornjs": "1.5.6",
    "prelude-ls": "1.1.1",
    "stats.js": "*",
    "localforage": "1.2.1",
    "backbone-fsm": "0.0.1",
    "bluebird": "2.7.1",
    "backbone.asyncstatemachine": "0.3.0",
    "moment": "2.9.0",
    "lodash": "3.3.1",
    "backbone-deep-model": "https://github.com/kahwee/backbone-deep-model.git#1.0.0",
    "pixi": "4.0.3",
    "backbone-react-component": "0.10",
    "classnames": "2.1.3",
    "base64-js": "1.0.0",
    "text-encoder-lite": "1.0.0"
  },
  "overrides": {
    "underscore": {
      "ignore": true
    },
    "backbone": {
      "main": "backbone.js",
      "dependencies": {
        "lodash": "*",
        "jquery": "*"
      }
    },
    "backbone-react-component": {
      "main": "lib/component.js",
      "dependencies": {
        "backbone": "1.2.0",
        "react": "0.13.0",
        "lodash": "*"
      }
    },
    "react": {
      "main": [
        "react-with-addons.js",
        "react-dom.js"
      ]
    },
    "modernizr": {
      "main": [
        "modernizr.js",
        "feature-detects/css-calc.js",
        "feature-detects/css-boxsizing.js"
      ]
    },
    "prelude-ls": {
      "main": "browser/prelude-browser.js"
    },
    "codemirror": {
      "main": [
        "lib/codemirror.js",
        "mode/xml/xml.js",
        "mode/css/css.js",
        "mode/htmlmixed/htmlmixed.js"
      ]
    },
    "stats.js": {
      "main": "build/stats.min.js"
    },
    "async": {
      "main": "lib/async.js"
    },
    "popcornjs": {
      "main": [
        "popcorn.js",
        "plugins/subtitle/popcorn.subtitle.js",
        "plugins/code/popcorn.code.js"
      ]
    },
    "font-awesome": {
      "main": "*.ignore"
    },
    "nouislider": {
      "main": "jquery.nouislider.min.js"
    },
    "handlebars": {
      "main": "handlebars.runtime.min.js"
    },
    "backbone-deep-model": {
      "main": "dist/backbone-deep-model.js",
      "dependencies": {
        "backbone": "*"
      }
    },
    "backbone.asyncstatemachine": {
      "main": "backbone.asyncstatemachine.js",
      "dependencies": {
        "backbone": "*",
        "bluebird": "*"
      }
    },
    "pixi": {
      "main": "bin/pixi.js"
    }
  },
  "resolutions": {
    "codemirror": "5.9.0",
    "backbone": "1.2.0",
    "backbone-react-component": "0.10",
    "react": "0.14.0",
    "underscore": "1.8.3"
  }
}
